{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/analyze",
      "methods": ["POST"],
      "dest": "api/main.py"
    },
    {
      "src": "/",
      "methods": ["GET"],
      "dest": "api/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/main.py"
    }
  ]
}